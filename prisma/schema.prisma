generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id   Int    @id @default(autoincrement())
  name String
  fees Fee[]
}

model Fee {
  id      Int    @id @default(autoincrement())
  comment String
  amount  Int
  doneBy  String
  user    User   @relation(fields: [userId], references: [id])
  userId  Int
  addedBy String

  @@index([userId])
}
